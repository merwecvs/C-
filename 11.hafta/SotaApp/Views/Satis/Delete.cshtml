@model SotaApp.Data.Satis
<p>
    <b>
        
        @Model.SatisId</b> numaralı satışı silmek istediğinize emin misiniz?
        <form asp-action="Delete" method="post">
            <input type="hidden" asp-asp-for="@Model.SatisId">
            <button type="submit" class="btn btn-danger">Evet</button>
            <a asp-action="Index" class="btn btn-primary">Hayır</a>
        </form>
</p>