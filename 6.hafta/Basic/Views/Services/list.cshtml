@model List<Services>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    @foreach(var hizmet in Model){
        <div id="Hizmet_@hizmet.Id">
            <h2>@hizmet.Title</h2> 
            <p>@hizmet.Description</p>
        </div>
    }
</body>
</html>    